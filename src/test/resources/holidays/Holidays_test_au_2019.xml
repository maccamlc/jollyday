                   xsi:schemaLocation="http://www.example.org/Holiday ../../../main/xsd/Holiday.xsd">
    <tns:Holidays>
        <!-- Australia Day -->
        <tns:Fixed month="JANUARY" day="28" descriptionPropertiesKey="NATIONAL_DAY"/>
        <!-- Good Friday -->
        <tns:Fixed month="APRIL" day="19" descriptionPropertiesKey="GOOD_FRIDAY"/>
        <!-- Easter Saturday -->
        <tns:Fixed month="APRIL" day="20" descriptionPropertiesKey="EASTER_SATURDAY"/>
