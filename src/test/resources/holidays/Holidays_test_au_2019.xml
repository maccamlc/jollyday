            <tns:Fixed month="MARCH" day="11" descriptionPropertiesKey="CANBERRA_DAY"/>
            <!-- Queen's Birthday -->
            <tns:Fixed month="JUNE" day="10" descriptionPropertiesKey="QUEENS_BIRTHDAY"/>
            <!-- Labour Day -->
            <tns:Fixed month="OCTOBER" day="7" descriptionPropertiesKey="LABOUR_DAY"/>
